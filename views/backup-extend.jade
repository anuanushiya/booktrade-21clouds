      // ===============================================
      // Content Wrapper. Contains page content
      .content-wrapper
        // Content Header (Page header)
        section.content-header
          h1
            | Sidebar Collapsed
            small Layout with collapsed sidebar on load
          ol.breadcrumb
            li
              a(href='#')
                i.fa.fa-dashboard
                |  Home
            li
              a(href='#') Layout
            li.active Collapsed Sidebar
        // Main content
        section.content
          .callout.callout-info
            h4 Tip!
            p
              | Add the sidebar-collapse class to the body tag to get this layout. You should combine this option with a
              | fixed layout if you have a long sidebar. Doing that will prevent your page content from getting stretched
              | vertically.
          // Default box
          .box
            .box-header.with-border
              h3.box-title Title
              .box-tools.pull-right
                button.btn.btn-box-tool(type='button', data-widget='collapse', data-toggle='tooltip', title='Collapse')
                  i.fa.fa-minus
                button.btn.btn-box-tool(type='button', data-widget='remove', data-toggle='tooltip', title='Remove')
                  i.fa.fa-times
            .box-body
              | Start creating your amazing application!
            // /.box-body
            .box-footer
              | Footer
            // /.box-footer
          // /.box
        // /.content
      // /.content-wrapper
